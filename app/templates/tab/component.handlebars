<div class="component component-tabs" data-widget="switchable" data-widget-autoplay="false" data-widget-trigger-type="click" data-widget-class-prefix="ue-tabs">
    <div class="hd">
        <ul class="tab-holders" data-role="tabs">
            {{#each data}}
                <li class="tab-item"><a class="tab-item-link" data-role="tab" href="{{url}}">{{title}}</a></li>
            {{/each}}
        </ul>
    </div>
    <div class="bd">
        {{#each data}}
            <div class="tab-panel" data-role="pane" cms-node-type="col">{{{content}}}</div>
        {{/each}}
    </div>
</div>